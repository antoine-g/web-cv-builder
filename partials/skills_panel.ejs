<div class="col-lg-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <span class="glyphicon <%= panel.glyphicon %>"></span><%= panel.title %>
      </div>
      <div class="panel-body">
          <% panel.body.forEach(function(i){ %>
            <div class="skill">
              <div class="skill_name"><%= i[0] %></div>
              <div class="progress">
                <% for(j=1; j<=i[1]; j++){ %>
                <div class="progress-bar progress-<%= j%>" role="progressbar"
                  aria-valuenow="1" aria-valuemin="0"
                  aria-valuemax="5" style="width: 19%;"></div>
                <% }; %>
              </div>
            </div>
          <% }); %>
      </div>
    </div>
  </div>